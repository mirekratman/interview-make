{
	"name": "cotd",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "concurrently --kill-others \"npm run server:run\" \"ng serve --proxy-config proxy.conf.json\"",
		"build": "ng build --aot --output-path=dist/client && npm run server:build",
		"server:run": "nodemon",
		"server:build": "tsc -p ./server",
		"watch": "ng build --watch --configuration development",
		"test": "jest",
		"postinstall": "npm run build"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~15.1.1",
		"@angular/common": "~15.1.1",
		"@angular/compiler": "~15.1.1",
		"@angular/core": "~15.1.1",
		"@angular/forms": "~15.1.1",
		"@angular/platform-browser": "~15.1.1",
		"@angular/platform-browser-dynamic": "~15.1.1",
		"@angular/router": "~15.1.1",
		"@fortawesome/angular-fontawesome": "^0.12.1",
		"@fortawesome/fontawesome-svg-core": "^1.2.35",
		"@fortawesome/free-brands-svg-icons": "^5.15.3",
		"@fortawesome/free-regular-svg-icons": "^5.15.3",
		"@fortawesome/free-solid-svg-icons": "^5.15.3",
		"@ngxs/devtools-plugin": "^3.7.6",
		"@ngxs/store": "^3.7.6",
		"bootstrap": "^5.0.1",
		"express": "^4.17.1",
		"express-session": "^1.17.3",
		"immer": "^9.0.19",
		"popper.js": "^1.16.1",
		"rxjs": "~7.8.0",
		"tslib": "^2.1.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~15.1.2",
		"@angular/cli": "~15.1.2",
		"@angular/compiler-cli": "~15.1.1",
		"@types/express": "^4.17.13",
		"@types/jest": "^28.0.0",
		"@types/node": "^12.11.1",
		"concurrently": "^6.4.0",
		"jasmine-core": "~3.7.0",
		"jest": "^28.0.0",
		"jest-preset-angular": "^12.0.0",
		"nodemon": "^1.19.4",
		"ts-node": "^10.9.1",
		"typescript": "~4.9.4"
	}
}
